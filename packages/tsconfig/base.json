{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2020",
      "DOM"
    ],
    "module": "es2020",
    "moduleResolution": "node",
    "noUnusedParameters": false,
    "outDir": "./build",
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "strictNullChecks": false,
    "target": "es2020"
  }
}
